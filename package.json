{
    "name": "lite-front",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "postinstall": "npm run prepare",
        "build": "cross-env NODE_ENV=production next build",
        "start:dev": "next",
        "start:prod": "cross-env NODE_ENV=production npx env-cmd --silent next start",
        "ts:check": "tsc --noEmit",
        "lint": "./node_modules/.bin/eslint .",
        "lint:fix": "./node_modules/.bin/eslint . --fix",
        "check": "npm run ts:check && npm run lint",
        "gen": "graphql-codegen --require dotenv/config --config codegen.yml",
        "update": "npx ncu -u && npx rimraf node_modules package-lock.json && npm i --legacy-peer-deps",
        "analyze": "cross-env ANALYZE=true next build",
        "prepare": "husky install"
    },
    "dependencies": {
        "@apollo/client": "^3.7.16",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@fontsource/roboto": "^5.0.3",
        "@mui/icons-material": "^5.11.16",
        "@mui/material": "^5.13.6",
        "@mui/styled-engine-sc": "^5.12.0",
        "@next/bundle-analyzer": "^13.4.7",
        "@sentry/nextjs": "^7.56.0",
        "@vercel/og": "^0.5.8",
        "apollo3-cache-persist": "^0.14.1",
        "dotenv": "^16.3.1",
        "env-cmd": "^10.1.0",
        "graphql": "^16.7.1",
        "next": "13.4.7",
        "next-compose-plugins": "^2.2.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sharp": "^0.32.1",
        "styled-components": "^6.0.0",
        "zustand": "^4.3.8"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^5.0.0",
        "@graphql-codegen/cli": "4.0.1",
        "@graphql-codegen/schema-ast": "^4.0.0",
        "@graphql-codegen/typescript": "4.0.1",
        "@graphql-codegen/typescript-operations": "4.0.1",
        "@graphql-codegen/typescript-react-apollo": "3.3.7",
        "@jetbrains/eslint-config": "^5.4.1",
        "@types/node": "^20.3.2",
        "@types/react": "^18.2.14",
        "@types/react-dom": "^18.2.6",
        "@types/styled-components": "^5.1.26",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "cross-env": "^7.0.3",
        "eslint": "8.43.0",
        "eslint-config-next": "^13.4.7",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-comments": "^1.0.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-no-secrets": "^0.8.9",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-security": "^1.7.1",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-sonarjs": "^0.19.0",
        "eslint-plugin-unicorn": "^47.0.0",
        "husky": "^8.0.3",
        "npm-check-updates": "^16.10.13",
        "prettier": "^2.8.8",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.5"
    }
}
