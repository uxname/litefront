{
  "name": "vitefront",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run check && tsc && vite build",
    "start:dev": "vite",
    "start:prod": "vite preview",
    "gen": "graphql-codegen --require dotenv/config --config codegen.yml",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:style": "stylelint \"**/*.{css,scss}\"",
    "lint:style:fix": "npm run lint:style -- --fix",
    "lint:staged": "lint-staged",
    "ts:check": "tsc --noEmit",
    "check": "npm run lint:style && npm run ts:check && npm run lint",
    "update": "npx ncu -u && npx rimraf node_modules package-lock.json && npm i --legacy-peer-deps && npm audit fix && npm run check",
    "prepare": "husky",
    "postinstall": "npm run prepare"
  },
  "dependencies": {
    "@apollo/client": "^3.8.10",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-codegen/typescript-react-apollo": "4.1.0",
    "@tanstack/router-devtools": "^1.15.4",
    "dotenv": "^16.4.1",
    "graphql": "^16.8.1",
    "npm-check-updates": "^16.14.14",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-pwa": "^0.17.5",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@jetbrains/eslint-config": "^5.4.1",
    "@tanstack/router-vite-plugin": "^1.15.4",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-comments": "^1.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "husky": "^9.0.7",
    "lint-staged": "^15.2.0",
    "sass": "^1.70.0",
    "sharp": "^0.33.2",
    "stylelint": "^16.2.0",
    "stylelint-config-standard": "^36.0.0",
    "svgo": "^3.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-image-optimizer": "^1.1.7"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "npm run lint:fix",
    "*.{css,scss}": "npm run lint:style:fix",
    "*.{json,md,html,xml,yml,yaml,mdx}": "prettier --write"
  }
}
