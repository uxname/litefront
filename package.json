{
  "name": "lite-front",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "________________ BUILD AND RUN ________________": "",
    "postinstall": "npm run prepare",
    "build": "cross-env NODE_ENV=production next build",
    "start:dev": "next",
    "start:prod": "cross-env NODE_ENV=production npx env-cmd --silent next start",
    "analyze": "cross-env ANALYZE=true next build",
    "________________ FORMAT AND LINT ________________": "",
    "ts:check": "tsc --noEmit",
    "lint": "./node_modules/.bin/eslint .",
    "lint:fix": "./node_modules/.bin/eslint . --fix",
    "check": "npm run ts:check && npm run lint",
    "________________ OTHER ________________": "",
    "gen": "graphql-codegen --require dotenv/config --config codegen.yml",
    "update": "npx ncu -u && npx rimraf node_modules package-lock.json && npm i --legacy-peer-deps",
    "prepare": "husky install",
    "extract-translations": "npx i18next 'src/**/*.{tsx,ts}' -c i18next-parser.config.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.8.4",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@hookform/resolvers": "^3.3.1",
    "@mui/icons-material": "^5.14.11",
    "@mui/material": "^5.14.11",
    "@mui/styled-engine-sc": "^5.14.11",
    "@next/bundle-analyzer": "^13.5.3",
    "@sentry/nextjs": "^7.72.0",
    "@vercel/og": "^0.5.17",
    "apollo3-cache-persist": "^0.14.1",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "graphql": "^16.8.1",
    "i18next": "^23.5.1",
    "next": "13.5.3",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^14.0.3",
    "next-language-detector": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.2",
    "react-i18next": "^13.2.2",
    "sharp": "^0.32.6",
    "styled-components": "^6.0.8",
    "uuid": "^9.0.1",
    "yup": "^1.3.2",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-codegen/typescript-react-apollo": "4.0.0",
    "@jetbrains/eslint-config": "^5.4.1",
    "@types/node": "^20.7.2",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@types/styled-components": "^5.1.28",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "cross-env": "^7.0.3",
    "eslint": "8.50.0",
    "eslint-config-next": "^13.5.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-comments": "^1.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": "^8.0.3",
    "i18next-parser": "^8.7.0",
    "npm-check-updates": "^16.14.4",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}

